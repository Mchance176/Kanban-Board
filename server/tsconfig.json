{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "rootDir": "src",
      "outDir": "dist",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "noImplicitReturns": false,
      "allowJs": true,
      "checkJs": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "typeRoots": ["./node_modules/@types", "./src/types"]
}
